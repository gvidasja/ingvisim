  // du Fibonačio sekos variantai
  fun loopedFibonacci(index){
    if{index < 3}{
      1
    }else{
      a1 = 1
      a2 = 1
      i = 3
      while{i <= index}{
        tmp = a1 + a2 + 0
        a1 = a2 + 0
        a2 = tmp + 0
        i = i + 1
      }
      a2
    }
  }

  fun fibonacci(n) {
    if {n == 0 || n == 1} {n}
    else
    {fibonacci(n - 1) + fibonacci(n - 2)}
  }

  print("fibonacci:")
  print(fibonacci(7))

  print("fibonacci:")
  print(loopedFibonacci(7))
}